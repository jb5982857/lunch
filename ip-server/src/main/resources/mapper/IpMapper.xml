<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lunch.ip.dao.IpDao">
    <select id="getStateByIp" resultType="java.lang.Integer">
        SELECT `state` FROM `ip` WHERE `ip` = #{ip}
    </select>
</mapper>